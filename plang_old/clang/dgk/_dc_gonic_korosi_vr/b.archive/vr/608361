<script>
if(typeof wif_adx != 'undefined') {
  var dc_adx = {
    listenMessage: function(e) {
      try {
        var d = $.parseJSON(e.data);
        switch (d.result) {
          case 'end':
            wif_adx.exitBanner();
            break;
        }
      } catch(err) {
        //console.log(err);
      }
    }
  };
  
  if (window.removeEventListener) {
    window.removeEventListener('message', wif_adx.listenMessage, false);
  } else {
    window.detachEvent('onmessage', wif_adx.listenMessage);
  }

  if (window.addEventListener) {
    window.addEventListener('message', dc_adx.listenMessage, false);
  } else {
    window.attachEvent('onmessage', dc_adx.listenMessage);
  }
}
</script>
