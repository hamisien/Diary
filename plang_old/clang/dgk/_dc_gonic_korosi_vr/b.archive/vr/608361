<div class="stickyunit" style="position:absolute;left:0;top:0;padding:186px 20px 0px 0px;width:120px;height:600px;z-index:1">
	<script type="text/javascript" src="https://addc.dcinside.com/NetInsight/js/dcinside/pc/body@left_game"></script>	<script type="text/javascript" charset="utf-8">
	    var stickyTop = $('.stickyunit').offset().top;       // get initial position of the element
	    $(window).scroll(function() {                  // assign scroll event listener
		    var currentScroll = $(window).scrollTop() - 156;
		    if(currentScroll <= stickyTop){
		        $('.stickyunit').css({                      // if you scroll above it
		            position: 'absolute',
		            top: '0',
		            left: '0',
		            padding: "186px 20px 0px 0px"
		        });
		    }else{
		        $('.stickyunit').css({                      // scroll to that element or below it
		            position: "absolute",
		            top: currentScroll,
		            padding: "20px 20px 0px 0px"
		        });
		    }
	    });
	</script>
</div>
